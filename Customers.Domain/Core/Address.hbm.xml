<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Customers.Domain.Core" assembly="Customers.Domain">
  <class name="Address" mutable="false">
    <id name="Id">
      <generator class="hilo">
      </generator>
    </id>
    <property name="Street" not-null="true" length="200"></property>
    <property name="Town" not-null="true" length="200"></property>
    <property name="ZipCode" not-null="true" length="10"></property>
    <property name="Country" not-null="true" length="50"></property>
  </class>
</hibernate-mapping>